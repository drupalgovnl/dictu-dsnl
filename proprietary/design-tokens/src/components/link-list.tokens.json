{
  "govnl": {
    "link-list": {
      "row-gap": {
        "$type": "dimension",
        "$value": "{govnl.spacing.gap.sm}"
      },
      "icon": {
        "inset-block-start": {
          "$type": "dimension",
          "$value": "0px"
        },
        "size": {
          "$type": "dimension",
          "$value": "{govnl.sizing.icon.md}"
        }
      },
      "link": {
        "column-gap": {
          "$type": "dimension",
          "$value": "{govnl.spacing.gap.sm}"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "None"
        }
      },
      "font-weight": {
        "$type": "fontWeights",
        "$value": "{govnl.document.font-weight}"
      }
    }
  },
  "todo": {
    "link-list": {
      "link": {
        "hover": {
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "underline"
          }
        }
      }
    }
  }
}
